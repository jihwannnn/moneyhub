<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:clipToPadding="false"
        android:id="@+id/raj0hzffn35b"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="#FFFFFF"
        android:paddingTop="48dp">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/r6gd0ntwrhza"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="18dp"
                android:layout_marginHorizontal="68dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="money"
                    android:textColor="#FFE36E"
                    android:textSize="40sp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="hub"
                    android:textColor="#6ED1D3"
                    android:textSize="40sp"
                    android:textStyle="bold" />


<!--                <ImageView-->
<!--                    android:id="@+id/ro8h7w216p4"-->
<!--                    android:layout_width="200dp"-->
<!--                    android:layout_height="39dp"-->
<!--                    android:src="@mipmap/ic_launcher"-->
<!--                    android:scaleType="fitXY"-->
<!--                    />-->


                <View
                    android:layout_width="22dp"
                    android:layout_height="6dp"
                    android:layout_weight="1"
                    />

                <ImageView
                    android:id="@+id/rlyelz8l3ob"
                    android:layout_width="43dp"
                    android:layout_height="43dp"
                    android:src="@drawable/mypage"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rwulmjdm442"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#000000"
                android:layout_marginBottom="23dp"
                android:orientation="vertical">
            </LinearLayout>
            <TextView
                android:id="@+id/rpukn3o5jt6"
                android:textColor="#000000"
                android:textSize="24sp"
                android:textStyle="bold"
                android:layout_marginBottom="32dp"
                android:layout_marginStart="34dp"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="모임 이름"
                />
            <LinearLayout
                android:id="@+id/r2q551lhv7ib"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:background="@drawable/cr20be8e8e899"
                android:paddingVertical="12dp"
                android:paddingLeft="24dp"
                android:paddingRight="11dp"
                android:layout_marginBottom="30dp"
                android:layout_marginHorizontal="34dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/rta4dj3gt7p"
                    android:layout_width="41dp"
                    android:layout_height="50dp"
                    android:src="@drawable/group"
                    android:scaleType="fitXY"
                    />

                <EditText
                    android:id="@+id/groupEditText"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:hint="모임이름"
                    android:inputType="textEmailAddress"
                    android:textColor="#000000"
                    android:textSize="20sp" />



                <LinearLayout
                    android:id="@+id/rpdbebaurxfb"
                    android:layout_width="74dp"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:background="@drawable/small_but"
                    android:paddingVertical="10dp"
                    android:elevation="4dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/rzff31h4ep8c"
                        android:textColor="#000000"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:text="중복확인"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rcd2czfcwgqn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:background="@drawable/cr20bffe36eb0"
                android:paddingVertical="23dp"
                android:layout_marginHorizontal="39dp"
                android:elevation="4dp"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/r5utuogl2s85"
                    android:textColor="#000000"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="Start"
                    />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>